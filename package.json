{
  "name": "forkify",
  "version": "1.0.0",
  "description": "Recipe application",
  "default": "index.html",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir ./dist",
    "predeploy": "parcel build ./*.html --public-url https://github.com/pranavmappoli/forkify",
    "build-github": "parcel build index.html --public-url ./ -d docs",
    "deploy": "gh-pages -d dist"
  },
  "author": "Pranav M",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.1",
    "parcel": "^2.0.0-beta.1",
    "parcel-plugin-data-src": "^1.0.1",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "fractional": "^1.0.0",
    "gh-pages": "^3.2.3",
    "regenerator-runtime": "^0.13.7"
  }
}
